---
source: crates/pixi_manifest/src/pypi/pypi_options.rs
expression: merged_opts
---
index-url: "https://example.com/pypi"
extra-index-urls:
  - "https://example.com/extra"
  - "https://example.com/extra2"
find-links:
  - path: /path/to/flat/index
  - url: "https://flat.index/"
  - path: /path/to/flat/index2
  - url: "https://flat.index2/"
no-build-isolation:
  - foo
  - bar
index-strategy: ~
prerelease-mode: ~
no-build: all
dependency-overrides:
  pkg1:
    version: "==1.0.0"
  pkg3:
    version: "==3.2.0"
  pkg2:
    version: "==2.0.0"
no-binary: ~
skip-wheel-filename-check: true
